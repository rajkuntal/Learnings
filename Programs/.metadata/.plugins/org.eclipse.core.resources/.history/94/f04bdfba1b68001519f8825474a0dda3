public class SortThreeElemArray {
	
	public void sort(int[] a)
	{
		int Fpos = 0, SPos = a.length;
		int j;
		for(int i = 0; i < a.length; i++)
		{
			if (a[i] == 2)
			{
				if(SPos == a.length && a[SPos] == 2)
				{
					for(j = a.length-2; j >= 0; j++)
					{
						if(a[SPos] != 2)
						{
							SPos = j;
							break;
						}
					}
				}
			}
		}
	}

}
