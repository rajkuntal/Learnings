
public class ProblemOne {

	public boolean goOn = true;
	static int s =0;
	
	static
	{
		increment();
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	public static void increment() {
		if(goOn)
		{
			for(int i = 0 ; i < 100; i++)
			{
				s = s + 1;
			}
			goOn = false;
		}
	}

}
