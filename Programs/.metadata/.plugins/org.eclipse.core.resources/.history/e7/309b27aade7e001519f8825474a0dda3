package graph;

public class GraphNode {
	int data;
	GraphNode neibhours[];
	int visited;
	
	public GraphNode(int d) {
		// TODO Auto-generated constructor stub
		this.data = d;
	}
	
	public GraphNode(GraphNode[] nbrs) {
		// TODO Auto-generated constructor stub
		this.neibhours = nbrs;
	}
	
	public GraphNode(int d, GraphNode[] nbrs) {
		// TODO Auto-generated constructor stub
		this.data = d;
		this.neibhours = nbrs;
	}
	
	public void toString() {
		System.out.println("Node Data :" + this.data);
	}
}
